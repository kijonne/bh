    <script>
        // Создаем таблицу 2x2
        document.write("<table>");
        document.write("<tr><td>Ячейка 1</td><td>Ячейка 2</td></tr>");
        document.write("<tr><td>Ячейка 3</td><td>Ячейка 4</td></tr>");
        document.write("</table>");
    </script>
</body>
